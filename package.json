        "prepare": "husky install",
        "format": "prettier --write \"./**/*.{html,vue,ts,js,json,md,less,sass,scss,sty}\"",
        "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
        "lint:css": "stylelint **/*.{vue,scss,css} --fix",
        "test": "vitest",
        "coverage": "vitest run --coverage",
